<h1> PRODUCTOS </h1>
<div class="dataUserContainer">
    <div class="hGroup">
        <h4> Bienvenido {{data.dataLoginUser.name}}</h4>
        <h4> You are an: {{data.dataLoginUser.rol}}</h4>
    </div>
    <div>
        <form action="/api/sessions/logout">
            <button type="submit">Logout</button>
        </form>
    </div>
</div>

<div id="productContainer">
    {{#each data.arrayViewProd}}
        <div>
            <div>
                <ul>
                    
                    <li> <strong>Id:</strong> {{_id}} </li>
                    <li> Title: {{title}} </li>
                    <li> Description: {{description}} </li>
                    <li> Price: {{price}} </li>
                    <li> Code: {{code}} </li>
                    <li> Status: {{status}} </li>
                    <li> Category: {{category}} </li>
                    <li> Stock: {{stock}} </li>
                    <li> Thumbnail: {{thumbnail}} </li>
                    
                </ul>
            </div>               
        </div>
    {{/each}}
</div>
<link rel="stylesheet" href="/public/css/products.css">
<script src="/socket.io/socket.io.js"></script>
<script src="/js/products.js"></script>